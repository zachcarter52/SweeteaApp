<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.example.sweetea"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="25"
        android:targetSdkVersion="34" />

    <uses-feature
        android:name="android.hardware.microphone"
        android:required="true" />

    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" /> <!-- Starting with API 31, location-based Bluetooth perms are now classified as "dangerous". -->
    <!-- Any device on 30 and below will use the old permissions, and 31+ will use the new ones. -->
    <uses-permission
        android:name="android.permission.BLUETOOTH"
        android:maxSdkVersion="30" />
    <uses-permission
        android:name="android.permission.BLUETOOTH_ADMIN"
        android:maxSdkVersion="30" />
    <uses-permission android:name="android.permission.BLUETOOTH_CONNECT" />
    <uses-permission
        android:name="android.permission.BLUETOOTH_SCAN"
        android:usesPermissionFlags="neverForLocation" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.RECORD_AUDIO" />
    <uses-permission android:name="android.permission.HIDE_OVERLAY_WINDOWS" />
    <uses-permission android:name="android.permission.NFC" />
    <uses-permission android:name="android.permission.WAKE_LOCK" /> <!-- Package visibility restrictions are enforced for API 30 and above. -->
    <!-- Explicit queries are defined to limit visibility of intent actions to certain apps/packages. -->
    <!-- See: https://developer.android.com/training/package-visibility for more details. -->
    <!-- Note: This module is SPECIAL as it houses isIntentAvailable, which is called everywhere. -->
    <!-- We want to handle the common use cases here vs at call sites to avoid regressions. -->
    <queries>

        <!-- Make sure apps that allow us to view URLs (i.e. the browser) are visible. -->
        <intent>
            <action android:name="android.intent.action.VIEW" />

            <data android:scheme="https" />
        </intent>
        <!-- Make sure apps that allow us to open the Play Store are visible. -->
        <intent>
            <action android:name="android.intent.action.VIEW" />

            <data android:scheme="market" />
        </intent>
        <!-- Make sure each of the Square vertical packages are visible to the PackageManager. -->
        <package android:name="com.android.inputmethod.latin.square" />
        <package android:name="com.squareup" />
        <package android:name="com.squareup.apos" />
        <package android:name="com.squareup.bran" />
        <package android:name="com.squareup.camera" />
        <package android:name="com.squareup.invoicesapp" />
        <package android:name="com.squareup.oob" />
        <package android:name="com.squareup.restaurants" />
        <package android:name="com.squareup.retail" />
        <package android:name="com.squareup.signin" />
        <package android:name="com.squareup.squid.push" />
        <package android:name="com.squareup.squid.launcher" />
        <package android:name="com.squareup.switcher" />
        <package android:name="com.squareup.t2devicetour" />
        <package android:name="com.squareup.t3devicetour" />
        <package android:name="com.squareup.terminalapi" />
        <package android:name="com.squareup.x2devicetour" />
        <package android:name="com.squareup.x2settings" /> <!-- Beta packages too! We need to be explicit, unfortunately - wildcards do not work. -->
        <package android:name="com.android.inputmethod.latin.square.beta" />
        <package android:name="com.squareup.apos.beta" />
        <package android:name="com.squareup.beta" />
        <package android:name="com.squareup.camera.beta" />
        <package android:name="com.squareup.invoicesapp.beta" />
        <package android:name="com.squareup.oob.beta" />
        <package android:name="com.squareup.restaurants.beta" />
        <package android:name="com.squareup.retail.beta" />
        <package android:name="com.squareup.signin.beta" />
        <package android:name="com.squareup.squid.push.beta" />
        <package android:name="com.squareup.switcher.beta" />
        <package android:name="com.squareup.t2devicetour.beta" />
        <package android:name="com.squareup.t3devicetour.beta" />
        <package android:name="com.squareup.terminalapi.beta" />
        <package android:name="com.squareup.x2devicetour.beta" />
        <package android:name="com.squareup.x2settings.beta" />
    </queries>

    <uses-permission android:name="android.permission.VIBRATE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.RECEIVE_BOOT_COMPLETED" />
    <uses-permission android:name="android.permission.FOREGROUND_SERVICE" />

    <permission
        android:name="org.example.sweetea.DYNAMIC_RECEIVER_NOT_EXPORTED_PERMISSION"
        android:protectionLevel="signature" />

    <uses-permission android:name="org.example.sweetea.DYNAMIC_RECEIVER_NOT_EXPORTED_PERMISSION" />

    <application
        android:allowBackup="true"
        android:appComponentFactory="papa.PerfAppComponentFactory"
        android:debuggable="true"
        android:extractNativeLibs="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:resizeableActivity="false"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:testOnly="true"
        android:theme="@style/Theme.App.Starting" >
        <activity
            android:name="org.example.sweetea.MainActivity"
            android:configChanges="orientation|screenSize|screenLayout|keyboardHidden|mnc|colorMode|density|fontScale|fontWeightAdjustment|keyboard|layoutDirection|locale|mcc|navigation|smallestScreenSize|touchscreen|uiMode"
            android:exported="true"
            android:theme="@style/Theme.App.Starting" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <uses-library
            android:name="wear-sdk"
            android:required="false" />

        <meta-data
            android:name="shadow.com.bugsnag.android.square.API_KEY"
            android:value="3679dbdddec052c40c1525a57ceb9704" /> <!-- See com.squareup.bugsnag.config.BugsnagConfigurationBuilder -->
        <meta-data
            android:name="com.squareup.CONFIGURE_BUGSNAG_FOR_SQUARE_SDKS"
            android:value="true" />
        <meta-data
            android:name="com.squareup.BUGSNAG_SDK_VERSION_NAME"
            android:value="2.0.0-beta5" />
        <meta-data
            android:name="com.squareup.BUGSNAG_SDK_VERSION_CODE"
            android:value="20009999" />

        <activity
            android:name="com.squareup.sdk.mobilepayments.payment.manager.MobilePaymentsSdkPaymentActivity"
