            android:theme="@style/MobilePaymentsSdk.Theme" >
        </activity>
        <activity
            android:name="com.squareup.sdk.mobilepayments.settings.MobilePaymentsSdkSettingsActivity"
            android:exported="false"
            android:theme="@style/MobilePaymentsSdk.Theme" >
        </activity>
        <activity
            android:name="com.squareup.sdk.mobilepayments.payment.manager.MobilePaymentsSdkCardEntryActivity"
            android:exported="false"
            android:theme="@style/MobilePaymentsSdk.Theme.CardEntry"
            android:windowSoftInputMode="adjustResize" >
        </activity>

        <receiver
            android:name="com.squareup.cardreader.ble.lock.PosMarkerReceiver"
            android:exported="false" >
            <intent-filter>
                <action android:name="com.squareup.cardreader.ble.FAKE_INTENT" />
            </intent-filter>
        </receiver> <!-- For printing and cash drawers. -->
        <activity
            android:name="com.squareup.usb.UsbAttachedActivity"
            android:enabled="false"
            android:exported="false"
            android:theme="@android:style/Theme.Translucent.NoTitleBar" >
            <intent-filter>
                <action android:name="android.hardware.usb.action.USB_DEVICE_ATTACHED" />
            </intent-filter>

            <meta-data
                android:name="android.hardware.usb.action.USB_DEVICE_ATTACHED"
                android:resource="@xml/usb_device_filter" />
        </activity>
        <activity
            android:name="sqip.internal.CardEntryActivity"
            android:exported="false"
            android:theme="@style/sqip.Theme.CardEntry"
            android:windowSoftInputMode="adjustResize" >
        </activity>

        <provider
            android:name="sqip.internal.ContextCaptureContentProvider"
            android:authorities="org.example.sweetea.sqip"
            android:exported="false" />
